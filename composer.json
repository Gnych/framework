{
  "name": "lavender/framework",
  "description": "Lavender Ecommerce Framework",
  "keywords": ["lavender", "ecommerce", "open source", "php"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ryan Tulino",
      "email": "rtulino@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "autoload": {
    "files": [
      "src/Support/helpers/functions.php"
    ],
    "psr-4": {
      "Lavender\\": "src"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
